<I c="StreetConditionalLayerEffect" i="snippet" m="civic_policies.street_civic_policy_effect" n="streetConditionalLayerEffect_Modern_Industrial_Refinery" s="234449">
  <V n="active_street_enact_dialog" t="enabled">
    <V n="enabled" t="dialog_icon_label">
      <U n="dialog_icon_label">
        <E n="dialog_bg_style">BG_CELEBRATION_LARGE</E>
        <E n="dialog_style">LARGE_ICON_TEXT_HORIZONTAL</E>
